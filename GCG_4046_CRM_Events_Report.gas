Program.Sub.cmdPrint_Click.Start
F.Intrinsic.Control.SetErrorHandler("cmdPrint_Click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSQL.Declare(String)
V.Local.sDateRange.Declare(String)
V.Local.sCustRange.Declare(String)
V.Local.iK.Declare(Long)
V.Local.sRow.Declare(String)
V.Local.iOrd.Declare(Long)
V.Local.sRange.Declare(String)

V.uGlobal.uBI.Redim(-1,-1)
'hide our form
Gui.SS_Form1..Visible(False)
Function.Intrinsic.UI.InvokeWaitDialog("Processing...","Please wait...")

'DELETE FROM OUR BI TABLE
F.ODBC.Connection!conX.Execute("Delete From GCG_BI_CRM_Events")

'first get our BIRUNID
F.Global.BI.GetRunID(V.Global.iBIRUNID)

'then get log id
F.Global.BI.StartLogging(V.Global.iBIRUNID,-1,-1,"",V.Global.iBILOGID)

'this sub will grab all our events that match the user enetered criteria,
'then we are going to go to the check_security sub, and if they have a specific security we are going to flag any customers that they only have access to and leave them in the
'bi file for printing.
'E = CRM_Events Table
'Type = CRM_Event_Type table
'Status = CRM_Event_Status table
V.Local.sSQL.Set("Select E.CompID,E.Creator,e.Title,E.Description,Type.Value As TValue,Status.Value As SValue,E.Start_DateTime,E.End_DateTime,E.Priority, Case When E.Close_DateTime Is Null Then '0' Else '1' End As Closed_Flag From CRM_Events As E Left Join CRM_Event_Type As Type On E.ETYPE = Type.SEQ Left Join CRM_Event_Status AS Status On  E.EType = Status.Seq")
Function.Intrinsic.Control.If(V.Screen.SS_Form1!chkAllDates.Value,=,0)
	'if they don't have all dates checked we reference our date pickers.
	f.Intrinsic.String.Build("Cast(E.End_DateTime As Date) Between '{0}' And '{1}'",v.Screen.SS_Form1!dtpStart.Value.PervasiveDate,v.Screen.SS_Form1!dtpEnd.Value.PervasiveDate,v.Local.sDateRange)
	Function.Intrinsic.Control.Else
		V.Local.sDateRange.Set("")
Function.Intrinsic.Control.EndIf

'lets try to loop through our listview and build our cust range selection, then we hit the dtabase only once.
F.Intrinsic.Control.If(V.Screen.SS_Form1!chkAllCust.Value,<>,1)
	Gui.SS_Form1.lvwCusts.RetrieveAllListItems(V.Local.sRow)
	F.Intrinsic.Control.If(V.Local.srow.Trim,<>,"***NORETURN***")
		F.Intrinsic.String.Split(V.Local.sRow,"*!*",V.Local.sRow)
		F.Intrinsic.Control.For(V.Local.iK,0,V.Local.sRow.UBound,1)
