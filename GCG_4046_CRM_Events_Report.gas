Program.Sub.cmdAdd_Click.Start
F.Intrinsic.Control.SetErrorHandler("cmdAdd_Click_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
v.Local.sBCust.Declare(String)
v.Local.sECust.Declare(STring)

v.Local.sBCust.Set(v.Screen.SS_Form1!txtBegCust.text)
v.Local.sECust.Set(v.Screen.SS_Form1!txtEndCust.text)

F.Intrinsic.Control.If(v.Local.sBCust.Trim,=,"")
	gui.SS_Form1.txtBegCust.SetFocus
	f.Intrinsic.Control.ExitSub
F.Intrinsic.Control.ElseIf(v.Local.sECust.Trim,=,"")
	gui.SS_Form1.txtEndCust.SetFocus
	Function.Intrinsic.Control.ExitSub
F.Intrinsic.Control.Else
	'grab our stuff and add it to our list view.
	f.Intrinsic.Math.Add(V.Global.ikey,1,V.Global.ikey)
	Gui.SS_Form1.lvwCusts.AddListItem(V.Global.ikey,V.Screen.SS_Form1!txtBegCust.Text)
	gui.SS_Form1.lvwCusts.SetListItemSubItemText(V.Global.ikey,1,V.Screen.SS_Form1!txtEndCust.Text)
	Gui.SS_Form1.txtBegCust.Text("")
	Gui.SS_Form1.txtEndCust.Text("")
f.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdAdd_Click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_4046_CRM_Events_Report.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	f.Intrinsic.Control.CallSub(ss_form1_unload)
Function.Intrinsic.Control.EndIf
Program.Sub.cmdAdd_Click.End

